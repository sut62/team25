ass = \"mx-auto\" height=\"100%\" width=\"100%\">\n    <v-navigation-drawer \n    absolute\n    src=\"https://images.unsplash.com/photo-1484807352052-23338990c6c6?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80\"\n    width=\"100%\"\n    parmanent>\n\n  <v-container>\n    <v-card class=\"mx-auto\" max-width=\"350\" color=\"#424242\" :elevation=\"13\" dark>\n    <v-row justify=\"center\">\n      <v-toolbar-title>\n      <h1 class=\"font-weight-CONDENSED\" dark>Company Register</h1>\n      </v-toolbar-title>\n    </v-row>\n    </v-card>\n    <br>\n    <v-card max-width=\"900\" class=\"mx-auto\" color=\"#EEEEEE\" :elevation=\"7\">\n      <v-row>\n        <v-col cols=\"15\">\n          <v-form v-model=\"valid\" ref=\"form\">\n          \n            <v-row style=\"height: 95px;\" justify=\"center\">\n              <v-col cols=\"3\" sm=\"4\">\n                <v-text-field\n                  v-model=\"company.name\"\n                  :items=\"names\"\n                  color=\"#42A5F5\"\n                  label=\"Company Name\"\n                  class=\"font-weight-bold\"\n                  :rules=\"[(v) => !!v || 'Company Name is required']\"\n                  required\n                  placeholder\n                  filied\n                  loading\n                  dense\n                  clearable\n                  prepend-icon=\"mdi-domain\"\n                ></v-text-field>\n              </v-col>\n            </v-row>\n\n            <v-row style=\"height: 80px;\" justify=\"center\">\n              <v-col cols=\"3\" sm=\"3\">\n                <v-select\n                 class=\"font-weight-bold\"\n                  label=\"Company Size\"\n                  outlined\n                  loading\n                  color=\"#1E88E5\"\n                  v-model=\"company.companySizeId\"\n                  :items=\"sizes\"\n                  item-text=\"name\"\n                  item-value=\"id\"\n                  :rules=\"[(v) => !!v || 'Please select Size']\"\n                ></v-select>\n              </v-col>\n              \n                <v-col cols=\"4\">\n                  <v-select\n                    class=\"font-weight-bold\"\n                    label=\"Company Type\"\n                    outlined\n                    loading\n                    color=\"#1976D2\"\n                    v-model=\"company.companyTypeId\"\n                    :items=\"types\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    :rules=\"[(v) => !!v || 'Please select Type']\"\n                  ></v-select>\n                </v-col>\n\n\n                <v-col cols=\"3\">\n                  <v-select\n                    class=\"font-weight-bold\"\n                    label=\"Province\"\n                    outlined\n                    loading\n                    color=\"#1565C0\"\n                    v-model=\"company.provinceId\"\n                    :items=\"provinces\"\n                    item-text=\"name\"\n                    item-value=\"id\"\n                    :rules=\"[(v) => !!v || 'Please select Province']\"\n                  ></v-select>\n                </v-col>\n            </v-row>\n\n            <!-- email -->\n          \n            <v-row style=\"height: 95px;\" justify=\"center\">\n              <v-col cols=\"3\" sm=\"4\">\n                <v-text-field\n                  v-model=\"company.email\"\n                  :items=\"emails\"\n                  :rules=\"emailRules\"\n                  color=\"#0D47A1\"\n                  label=\"E-mail\"\n                  class=\"font-weight-bold\"\n                  placeholder\n                  filied\n                  loading\n                  dense\n                  clearable\n                  prepend-icon=\"mdi-at\"\n                ></v-text-field>\n              </v-col>\n            </v-row>\n\n        \n\n        <!-- password -->\n            <v-row style=\"height: 120px;\" justify=\"center\">\n              <v-col cols=\"3\" sm=\"5\">\n                <v-text-field\n                  color=\"#000000\"\n                  solo\n                  class=\"font-weight-bold\"\n                  label=\"Password\"\n                  v-model=\"password\"\n                  :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n                  :rules=\"[rules.required, rules.min]\"\n                  :type=\"show1 ? 'text' : 'password'\"\n                  hint=\"At least 8 characters\"\n                  prepend-icon=\"mdi-textbox-password\"\n                  required\n                  counter\n                  clearable\n                  rounded\n                  outlined\n                  @click:append=\"show1 = !show1\"\n                ></v-text-field>\n              </v-col>\n\n        <!-- ยืมยันpassword -->\n              <v-col cols=\"3\" sm=\"5\">\n                <v-text-field\n                  color=\"#000000\"\n                  solo\n                  class=\"font-weight-bold\"\n                  label=\"Confirm Password\"\n                  v-model=\"repassword\"\n                  :type=\"show2 ? 'text' : 'password'\"\n                  :append-icon=\"show2 ?  'mdi-eye' : 'mdi-eye-off'\"\n                  :rules=\"[rules.required,rules.checkpass]\"\n                  hint=\"At least 8 characters\"\n                  prepend-icon=\"mdi-textbox-password\"\n                  required\n                  counter\n                  clearable\n                  rounded\n                  outlined\n                  @click:append=\"show2 = !show2\"\n                ></v-text-field>\n              </v-col>\n            </v-row>  \n        \n        </v-form>\n      </v-col>\n    </v-row>\n    </v-card>\n    <br>\n    <v-row justify=\"center\">\n              <v-col cols=\"5000\" sm=\"1000\">\n                <div v-if=\"saveCompany\">\n                <v-btn rounded @click=\"saveCompany\" :class=\"{ grey: !valid, black: valid }\" :