/companySize\")\n        .then(response => {\n          this.sizes = response.data;\n          console.log(response.data);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n     saveCompany() {\n       if((this.password == this.repassword) && (this.password.length>=10 || this.repassword.length>=10)\n      ){\n      http\n        .post(\n          \"/company/\" +\n            this.company.name +\n            \"/\"+\n            this.company.email +\n            \"/\"+\n            this.password +\n            \"/\"+\n            this.company.companySizeId +\n            \"/\"+\n            this.company.provinceId +\n            \"/\" +\n            this.company.companyTypeId,\n          this.company\n        )\n        .then(response => {\n        console.log(response);\n        alert(\"บันทึกสำเร็จ\");\n        this.$router.push(\"/logincompany\");\n         })\n        .catch(e => {\n        console.log(e);\n        });\n        this.submitted = true;\n      }else{\n      alert(\"บันทึกไม่สำเร็จ\");\n      }\n        },\n\n    getCompanys() {\n      http\n        .get(\"/company\")\n        .then(response => {\n          this.items = response.data;\n          console.log(this.items);\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n      ViewRegisterData(){\n      this.$router.push(\"/registerdata\");\n    },\n    back() {\n      this.$router.push(\"/\");\n    },\n    refreshList() {\n      this.getCompanyTypes();\n      this.getProvinces();\n      this.getCompanySizes();\n      this.getCompanys();\n    }\n    /* eslint-enable no-console */\n  },\n  mounted() {\n    this.getTypes();\n    this.getProvinces();\n    this.getSizes();\n    this.getCompanys();\n  }\n};\n\n</script>"]}]}